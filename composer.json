{
    "name": "thoth-pub/thoth-client-php",
    "description": "PHP client for Thoth's GraphQL and REST APIs.",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "ThothApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ThothApi\\Tests\\": "tests/"
        }
    }
}
